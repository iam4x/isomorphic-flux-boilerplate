{
  "name": "isomorphic-flux-boilerplate",
  "version": "0.5.0",
  "description": "An ES7 isomorphic Flux/ReactJS boilerplate",
  "main": "server/index.js",
  "scripts": {
    "build": "rm -rf dist && BABEL_ENV=production babel-node ./node_modules/.bin/webpack --stats --config ./webpack/prod.config.js",
    "dev": "babel-node ./webpack/dev-server.js",
    "test": "NODE_ENV=test NODE_PATH=app nyc --cache --extension .jsx ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint app server webpack shared test --ext .jsx,.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iam4x/isomorphic-flux-boilerplate.git"
  },
  "homepage": "https://github.com/iam4x/isomorphic-flux-boilerplate",
  "bugs": "https://github.com/iam4x/isomorphic-flux-boilerplate/issues",
  "author": "iam4x",
  "license": "MIT",
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env",
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "server/**/*",
      "app/data/*"
    ]
  },
